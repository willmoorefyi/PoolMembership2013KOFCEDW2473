<html xmlns:c="http://java.sun.com/jsp/jstl/core"  
   xmlns:spring="http://www.springframework.org/tags" 
   xmlns:jsp="http://java.sun.com/JSP/Page"
   xmlns:form="http://www.springframework.org/tags/form"
   xmlns:tags="urn:jsptagdir:/WEB-INF/tags" >
   
   <jsp:output doctype-root-element="HTML" doctype-system="about:legacy-compat" />
   <jsp:directive.page contentType="text/html;charset=UTF-8" />  
   <head>  
      <tags:header headerTitle="Success"></tags:header>
      
      <![CDATA[
      <!-- HTML5 shim, for IE6-8 support of HTML5 elements, outside the taglib due to CDATA support -->
      <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
      <![endif]-->
      ]]>
   </head>
   <body class="body-blue">
      <div class="page-wrapper">
         <jsp:include page="include/header-kofc-pool.jspx"/>
         <div class="container">
            <div class="page-header">
               <h1>Success!</h1>
            </div>
            <div class="row-fluid">
               <div class="span12"><h3>Thank you for your payment!</h3></div>
            </div>
            <div class="row-fluid">
               <div class="span12">Once your account is verified you will receive a welcome packet with further information.</div>
            </div>
         </div>
      </div>
